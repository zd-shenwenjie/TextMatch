{
  "name": "text-match",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "productName": "Text Match",
    "appId": "com.wenjie.textmatch",
    "copyright": "Copyright Â© 2021 shenwenjie",
    "directories": {
      "output": "build"
    },
    "win": {
      "target": [
        "nsis"
      ]
    }
  },
  "devDependencies": {
    "electron": "^11.2.1",
    "electron-builder": "^22.9.1",
    "electron-rebuild": "^2.3.4"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "antd": "^4.11.2",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^1.2.0",
    "electron-store": "^7.0.0",
    "fs-extra": "^8.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "wait-on": "^5.2.1",
    "web-vitals": "^0.2.4",
    "xlsx": "^0.16.9"
  },
  "scripts": {
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "electron:start": "electron .",
    "dev": "concurrently \"wait-on http://localhost:3000 && npm run electron:start\" \"cross-env BROWSER=none npm run react:start\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "prepack": "npm run react:build",
    "predist": "npm run react:build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
